{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/set-cookie",
      "headers": [
        {
          "key": "Set-Cookie",
          "value": "session_id=12345; Path=/; HttpOnly; SameSite=None; Secure"
        }
      ]
    }
  ]
}
